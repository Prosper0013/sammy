{
  "name": "pnl-card-generator",
  "version": "1.0.0",
  "dependencies": {
    "canvas": "^2.11.2"
  },
  "engines": {
    "node": "18.x"
  }
}
{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOWALL"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self' 'unsafe-inline' 'unsafe-eval' data: blob: *;"
        }
      ]
    }
  ]
}
